  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:release": "npm run check:prod-env && npm run check:stripe-config && next build",
    "start": "next start",
    "lint": "eslint . --max-warnings=0",
    "check:prod-env": "node scripts/check-production-env.mjs",
